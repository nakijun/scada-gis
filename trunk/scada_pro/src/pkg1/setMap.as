{
	import mx.controls.Image;

	public class setMap extends Image
	{
		public function setMap(x:Number, y:Number, width:Number, height:Number, source:String)
		{
			this.x = x;
			this.y = y;
			this.width = width;
			this.height = height;
			this.source = source;
		}
	}
}